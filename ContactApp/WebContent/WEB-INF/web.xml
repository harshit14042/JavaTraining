  </servlet>
  <servlet-mapping>
    <servlet-name>viewCategoryWise</servlet-name>
    <url-pattern>/categories</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>removeContact</display-name>
    <servlet-name>removeContact</servlet-name>
    <servlet-class>com.training.servlets.removeContact</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>removeContact</servlet-name>
    <url-pattern>/remove</url-pattern>
  </servlet-mapping>
</web-app>