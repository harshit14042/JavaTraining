
import java.io.*;

public class ObjectHandling {
	
	public boolean saveObject(Object obj){
		
		boolean status = false;
		ObjectOutputStream outStream=null;
		try {
			outStream=new ObjectOutputStream(new FileOutputStream(new File("customer.ser")));
		} catch (Exception e) {
			e.printStackTrace();
		}finally{
			outStream.close();
		}
		
		return status;
	}
}
